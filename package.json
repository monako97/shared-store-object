{
  "name": "shared-store-object",
  "version": "1.4.1",
  "description": "Shared Store Object",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "swc src/index.ts -d lib --strip-leading-paths -C module.type=commonjs -C jsc.target=es5 -C jsc.loose=true -C jsc.minify.mangle=true -C jsc.minify.compress=true -C minify=true && tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll --coverage"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@moneko/common": "1.11.0",
    "use-sync-external-store": "1.6.0"
  },
  "devDependencies": {
    "@swc/cli": "0.7.8",
    "@swc/core": "1.15.0",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/use-sync-external-store": "1.5.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "files": [
    "lib",
    "README.md",
    "README.zh-CN.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monako97/shared-store-object.git"
  },
  "keywords": [
    "shared-store-object",
    "sso",
    "shared-store",
    "react state management",
    "use-sync-external-store"
  ],
  "author": "moneko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/monako97/shared-store-object/issues"
  },
  "homepage": "https://github.com/monako97/shared-store-object#readme"
}
